pipeline {
    agent { dockerfile true }
    stages {
        stage('Clone Repository') {
            steps {
                git url: 'https://github.com/TuVanNguyen/DevOpsTutorials.git'
            }
        }
        stage('Build Image') {
            steps {
                app = docker.build("DevOpsTutorial/Jenkins/myDockerfile")
            }
        }
    }
}
